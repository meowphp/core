{
    "name": "meow/di",
    "description": "Dependency injection container",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Meow\\DI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meow\\DI\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "May Meow",
            "email": "emma@themaymeow.com"
        }
    ],
    "require": {
        "php": "8.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit tests",
        "cs-check": "phpcs --standard=PSR12 src",
        "cs-fix": "phpcbf --standard=PSR12 src",
        "stan": "phpstan analyse"
    }
}
